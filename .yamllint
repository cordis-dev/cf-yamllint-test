---
rules:
  braces: disable
  brackets: disable
  colons: disable
  commas: disable
  comments: disable
  comments-indentation: disable
  document-end: disable
  document-start: disable
  empty-lines: disable
  empty-values: {forbid-in-block-mappings: true, forbid-in-flow-mappings: true}
  hyphens: disable
  indentation: disable
  key-duplicates:
    level: warning
  key-ordering: {}
  line-length: {max: 20, allow-non-breakable-words: false, allow-non-breakable-inline-mappings: false}
  new-line-at-end-of-file: disable
  new-lines: disable
  octal-values: {forbid-implicit-octal: true, forbid-explicit-octal: true}
  trailing-spaces:
    level: warning
  truthy: {}
  quoted-strings: disable
